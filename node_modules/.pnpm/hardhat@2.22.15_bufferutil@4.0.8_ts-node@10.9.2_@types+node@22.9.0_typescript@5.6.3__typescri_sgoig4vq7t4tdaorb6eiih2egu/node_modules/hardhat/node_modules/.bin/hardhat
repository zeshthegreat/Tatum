#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/hardhat@2.22.15_bufferutil@4.0.8_ts-node@10.9.2_@types+node@22.9.0_typescript@5.6.3__typescri_sgoig4vq7t4tdaorb6eiih2egu/node_modules/hardhat/internal/cli/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/hardhat@2.22.15_bufferutil@4.0.8_ts-node@10.9.2_@types+node@22.9.0_typescript@5.6.3__typescri_sgoig4vq7t4tdaorb6eiih2egu/node_modules/hardhat/internal/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/hardhat@2.22.15_bufferutil@4.0.8_ts-node@10.9.2_@types+node@22.9.0_typescript@5.6.3__typescri_sgoig4vq7t4tdaorb6eiih2egu/node_modules/hardhat/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/hardhat@2.22.15_bufferutil@4.0.8_ts-node@10.9.2_@types+node@22.9.0_typescript@5.6.3__typescri_sgoig4vq7t4tdaorb6eiih2egu/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/hardhat@2.22.15_bufferutil@4.0.8_ts-node@10.9.2_@types+node@22.9.0_typescript@5.6.3__typescri_sgoig4vq7t4tdaorb6eiih2egu/node_modules/hardhat/internal/cli/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/hardhat@2.22.15_bufferutil@4.0.8_ts-node@10.9.2_@types+node@22.9.0_typescript@5.6.3__typescri_sgoig4vq7t4tdaorb6eiih2egu/node_modules/hardhat/internal/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/hardhat@2.22.15_bufferutil@4.0.8_ts-node@10.9.2_@types+node@22.9.0_typescript@5.6.3__typescri_sgoig4vq7t4tdaorb6eiih2egu/node_modules/hardhat/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/hardhat@2.22.15_bufferutil@4.0.8_ts-node@10.9.2_@types+node@22.9.0_typescript@5.6.3__typescri_sgoig4vq7t4tdaorb6eiih2egu/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../internal/cli/bootstrap.js" "$@"
else
  exec node  "$basedir/../../internal/cli/bootstrap.js" "$@"
fi

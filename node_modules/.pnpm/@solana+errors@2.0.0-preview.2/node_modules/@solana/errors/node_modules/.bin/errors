#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/@solana+errors@2.0.0-preview.2/node_modules/@solana/errors/bin/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@solana+errors@2.0.0-preview.2/node_modules/@solana/errors/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@solana+errors@2.0.0-preview.2/node_modules/@solana/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@solana+errors@2.0.0-preview.2/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/codespaces-blank/node_modules/.pnpm/@solana+errors@2.0.0-preview.2/node_modules/@solana/errors/bin/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@solana+errors@2.0.0-preview.2/node_modules/@solana/errors/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@solana+errors@2.0.0-preview.2/node_modules/@solana/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/@solana+errors@2.0.0-preview.2/node_modules:/workspaces/codespaces-blank/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
